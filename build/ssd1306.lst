 233:Drivers/OLED/Src/ssd1306.c ****     
 234:Drivers/OLED/Src/ssd1306.c ****     // Check remaining space on current line
 235:Drivers/OLED/Src/ssd1306.c ****     if (SSD1306_WIDTH < (SSD1306.CurrentX + Font.FontWidth) ||
 236:Drivers/OLED/Src/ssd1306.c ****         SSD1306_HEIGHT < (SSD1306.CurrentY + Font.FontHeight))
 237:Drivers/OLED/Src/ssd1306.c ****     {
 238:Drivers/OLED/Src/ssd1306.c ****         // Not enough space on current line
 239:Drivers/OLED/Src/ssd1306.c ****         return 0;
 240:Drivers/OLED/Src/ssd1306.c ****     }
 241:Drivers/OLED/Src/ssd1306.c ****     

 200:Drivers/OLED/Src/ssd1306.c **** //    Draw one pixel in the screenbuffer
 201:Drivers/OLED/Src/ssd1306.c **** //    X => X Coordinate
 202:Drivers/OLED/Src/ssd1306.c **** //    Y => Y Coordinate
 203:Drivers/OLED/Src/ssd1306.c **** //    color => Pixel color
 204:Drivers/OLED/Src/ssd1306.c **** void ssd1306_DrawPixel(uint8_t x, uint8_t y, SSD1306_COLOR color) {
 205:Drivers/OLED/Src/ssd1306.c ****     if(x >= SSD1306_WIDTH || y >= SSD1306_HEIGHT) {
 206:Drivers/OLED/Src/ssd1306.c ****         // Don't write outside the buffer
 207:Drivers/OLED/Src/ssd1306.c ****         return;
 208:Drivers/OLED/Src/ssd1306.c ****     }
 209:Drivers/OLED/Src/ssd1306.c ****     
 210:Drivers/OLED/Src/ssd1306.c ****     // Check if pixel should be inverted
 211:Drivers/OLED/Src/ssd1306.c ****     if(SSD1306.Inverted) {
 212:Drivers/OLED/Src/ssd1306.c ****         color = (SSD1306_COLOR)!color;
 213:Drivers/OLED/Src/ssd1306.c ****     }
 214:Drivers/OLED/Src/ssd1306.c ****     
 215:Drivers/OLED/Src/ssd1306.c ****     // Draw in the right color
 216:Drivers/OLED/Src/ssd1306.c ****     if(color == White) {
 217:Drivers/OLED/Src/ssd1306.c ****         SSD1306_Buffer[x + (y / 8) * SSD1306_WIDTH] |= 1 << (y % 8);
 218:Drivers/OLED/Src/ssd1306.c ****     } else { 
 219:Drivers/OLED/Src/ssd1306.c ****         SSD1306_Buffer[x + (y / 8) * SSD1306_WIDTH] &= ~(1 << (y % 8));
 220:Drivers/OLED/Src/ssd1306.c ****     }
 221:Drivers/OLED/Src/ssd1306.c **** }
